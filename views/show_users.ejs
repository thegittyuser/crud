<%- include("partials/header");%>
<center>
  <table border="1" cellpadding="5">
    <tr>
      <th colspan="8">Users Records</th>
    </tr>
    <tr>
      <th>Username</th>
      <th>Email</th>
      <th>Password</th>
      <th>Phone</th>
      <th>Address</th>
      <th colspan="2">Action</th>
    </tr>

    <% userResult.forEach( (user) =>{%>
    <tr>
      <td><%= user.username %></td>
      <td><%= user.email %></td>
      <td><%= user.password %></td>
      <td><%= user.phone %></td>
      <td><%= user.address %></td>
      <td>
        <a href="/update_data/<%= user._id %>"><button>Edit</button></a>
      </td>
      <td>
        <a href="/delete_data/<%= user._id %>"><button>Delete</button></a>
      </td>
    </tr>
    <% });%>
  </table>
</center>
<%- include("partials/footer");%>
